{
  "name": "orpc-voting",
  "version": "0.0.0",
  "description": "Voting Support App Frontend",
  "main": "app/main.js",
  "repository": "https://github.com/hmalphettes/orpc-voting",
  "author": "orpc-it",
  "scripts": {
    "start": "node --harmony app/main.js",
    "prod": "./node_modules/.bin/pm2 start app/main.js --watch --node-args=--harmony",
    "postinstall": "./node_modules/.bin/jspm install",
    "resetdb": "node --harmony app/resetdb-cli.js"
  },
  "jspm": {
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "datatables": "github:DataTables/DataTables@^1.10.11",
      "handlebars": "github:components/handlebars.js@^4.0.3",
      "jquery": "github:components/jquery@^2.2.1",
      "jquery-ui": "github:components/jqueryui@^1.11.4",
      "pivottable": "github:nicolaskruchten/pivottable@2.0.2",
      "typeahead": "github:twitter/typeahead.js@^0.11.1",
      "webcamjs": "github:jhuckaby/webcamjs@^1.0.6"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      "github:nicolaskruchten/pivottable@2.0.2": {
        "dependencies": [
          "github:components/jquery@2.2.1"
        ]
      }
    }
  },
  "devDependencies": {
    "bluebird": "^3.0.5",
    "jspm": "^0.16.11",
    "standard": "*",
    "knuth-shuffle": "^1.0.1",
    "request-promise": "^1.0.2"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "json5": "^0.4.0",
    "koa": "^1.1.0",
    "koa-bodyparser": "^2.0.1",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-passport": "^1.2.0",
    "koa-route": "^2.4.2",
    "koa-router": "^5.2.3",
    "koa-session": "^3.3.1",
    "koa-static": "^1.4.9",
    "koa-websocket": "^1.1.0",
    "mysql": "^2.9.0",
    "passport-local": "^1.0.0",
    "pm2": "^0.15.7"
  }
